
public class Numbers {
	private int number, length;
	private int[] nums;
	
	public Numbers(){
		number = 0;
		length = 0;
		nums = new int[0];
	}
	public Numbers(int num){
		setNumbers(num);
	}
	public void setNumbers(int num){
		number = num;
		for (int i=number; i > number; i--){
			length += 1;
			nums[length-1]= (num % 10);
			num /= 10;
		}
	}
	public  boolean isGoofy(int num){
		int div = 0;
		for (int i=0; i < length; i++){
			if (number % nums[i] == 0){
				div += nums[i];
			}
		}
		if ((div%2) == 1){
			return true;
		}
		else{
			return false;
		}
	}
	public static int[] getSomeGoofyNumbers (int count){
		
	}
}
